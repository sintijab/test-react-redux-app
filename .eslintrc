{
  "parser": "babel-eslint",
  "rules": {
    "max-len": [1, 70, 2, {ignoreComments: true}],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": 0
    },
  "extends": ["react", "airbnb/hooks", "airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
